		"@noir-lang/backend_barretenberg": "^0.36.0",
		"@noir-lang/noir_js": "1.0.0-beta.15",
		"@noir-lang/noir_wasm": "1.0.0-beta.16",
		"@noir-lang/noirc_abi": "1.0.0-beta.16",
		"@rolldown/binding-linux-x64-gnu": "1.0.0-beta.59",
		"buffer": "^6.0.3",
		"imurmurhash": "^0.1.4",
		"multiformats": "^13.4.2",
		"node-localstorage": "^3.0.5",
		"pinata": "^2.5.2",
		"solc": "^0.8.28",
		"viem": "^2.41.2",
		"vite": "^7.2.7",
		"vite-plugin-node-polyfills": "0.17.0"
	},
	"pnpm": {
		"overrides": {
			"write-file-atomic": "7.0.0"
		},
		"patchedDependencies": {
			"@lit-protocol/networks@8.3.2": "patches/@lit-protocol__networks@8.3.2.patch"
		}
	}
