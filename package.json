{
	"name": "fangorn-sdk",
	"version": "0.0.1",
	"description": "A zero-knowledge conditional access control framework",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fangorn-network/fangorn.git"
	},
	"license": "MIT",
	"type": "module",
	"main": "lib/index.js",
	"files": [
		"lib/"
	],
	"scripts": {
		"build": "tsdown",
		"format": "prettier . --write",
		"lint": "eslint . --max-warnings 0",
		"prepare": "husky",
		"test": "dotenvx run -f .env -- vitest run",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.1",
		"@eslint/js": "9.39.1",
		"@release-it/conventional-changelog": "10.0.2",
		"@types/node": "24.10.1",
		"@vitest/coverage-v8": "4.0.15",
		"@vitest/eslint-plugin": "1.5.1",
		"blake3-js": "^0.0.5",
		"eslint": "9.39.1",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"patch-package": "^8.0.1",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "3.7.3",
		"release-it": "19.0.6",
		"tsdown": "0.16.8",
		"typescript": "5.9.3",
		"typescript-eslint": "8.48.1",
		"vitest": "4.0.15",
		"write-file-atomic": "5.0.1"
	},
	"packageManager": "pnpm@10.4.0",
	"engines": {
		"node": ">=20.19.0"
	},
	"dependencies": {
		"@aztec/bb.js": "3.0.0-rc.2",
		"@lit-protocol/access-control-conditions": "^8.0.2",
		"@lit-protocol/auth": "^8.1.2",
